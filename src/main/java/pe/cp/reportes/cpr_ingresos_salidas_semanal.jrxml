		</variable>
		<variable name="total_salida" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{sum(d.SALIDAS)}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
