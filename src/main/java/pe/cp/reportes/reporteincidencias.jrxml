	<queryString>
		<![CDATA[SELECT DATE_FORMAT(OP.FECOPERACION,'%d/%m/%Y') AS FECHA_INCIDENCIA,
       DATE_FORMAT(OP.FECOPERACION,'%H:%i:%s %p') AS HORA_INCIDENCIA,
       TINC.DESCTIPOINC AS NOVEDAD,
       INC.DESINCIDENCIA AS ACCION_TOMADA,
       C.RAZONSOCIAL,
       UO.NOMBRE
FROM
INCIDENCIA INC,
TIPOINCIDENCIA TINC,
OPERACION OP,
UNIDADOPERATIVA UO,
CLIENTE C
WHERE INC.IDTIPOINC = TINC.IDTIPOINC AND
INC.IDOPERACION = OP.IDOPERACION AND
OP.IDUNIDAD = UO.IDUNIDAD AND
UO.IDCLIENTE = C.IDCLIENTE AND
UO.IDCLIENTE = $P{P_ID_CLIENTE} AND
OP.IDUNIDAD = $P{P_ID_UNIDAD}]]>
	</queryString>
